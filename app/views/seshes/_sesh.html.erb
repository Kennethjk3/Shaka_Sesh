<div class="media">
  <div class="media-left">
    <%= link_to image_tag(sesh.image.url(:thumb), class: 'media-object'), sesh.image.url, target: '_blank' %>
  </div>
  <div class="media-body">
    <h4 class="media-heading"><%= sesh.title %></h4>
    <%= link_to 'Remove', sesh_path(sesh), class: 'btn btn-danger', method: :delete, data: {confirm: 'Are you sure?'} %>
  </div>
</div>
